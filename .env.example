# LLM API Configuration
LLM_INFER_URL=https://api.kindroid.ai/v1/discord-bot  
LLM_API_KEY=your_kindroid_api_key_here

# Bot Configurations
# Add as many numbered sets as needed (1, 2, 3, etc.) 
# Required for each bot:
# - SHARED_AI_CODE_N: The shared AI code for API calls
# - BOT_TOKEN_N: Discord bot token
# Optional for each bot:
# - ENABLE_FILTER_N: Enable NSFW filtering

# Bot 1 Configuration
SHARED_AI_CODE_1=your_shared_code_1_here
BOT_TOKEN_1=your_discord_bot_token_1_here
ENABLE_FILTER_1=false  # Optional: Enable NSFW filtering.

# Bot 2 Configuration, add more by incrementing the _number
# SHARED_AI_CODE_2=your_shared_code_2_here
# BOT_TOKEN_2=your_discord_bot_token_2_here
# ENABLE_FILTER_2=false